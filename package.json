{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "npm run prisma:generate && remix build",
    "dev": "node -r dotenv/config ./node_modules/.bin/remix dev",
    "postinstall": "remix setup node && npm run prisma:generate",
    "start": "cross-env NODE_ENV=production node -r dotenv/config ./node_modules/.bin/remix-serve build",
    "prisma:generate": "npx prisma generate --schema=./app/repositories/main-repository/prisma/schema.prisma",
    "prisma:reset": "npx prisma migrate reset --schema=./app/repositories/main-repository/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=./app/repositories/main-repository/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=./app/repositories/main-repository/prisma/schema.prisma",
    "prisma:format": "npx prisma format --schema=./app/repositories/main-repository/prisma/schema.prisma",
    "prisma:validate": "npx prisma validate --schema=./app/repositories/main-repository/prisma/schema.prisma",
    "db:start": "docker compose up -d postgres",
    "db:stop": "docker compose stop postgres",
    "typecheck": "tsc --project ./tsconfig.json"
  },
  "dependencies": {
    "@nextui-org/react": "^2.1.13",
    "@prisma/client": "^5.3.1",
    "@remix-run/node": "^2.0.0",
    "@remix-run/react": "^2.0.0",
    "@remix-run/serve": "^2.0.0",
    "@remix-run/server-runtime": "^2.0.0",
    "@smplrspace/smplr-loader": "^2.9.0",
    "@types/pluralize": "^0.0.30",
    "@uidotdev/usehooks": "^2.3.1",
    "@vercel/remix": "^2.0.0",
    "chalk": "^5.3.0",
    "clsx": "^2.0.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.16.2",
    "isbot": "latest",
    "lucide-react": "^0.272.0",
    "nanoid": "^5.0.1",
    "pluralize": "^8.0.0",
    "prisma": "^5.3.1",
    "ramda": "^0.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix": "^2.0.0",
    "remix-auth": "^3.5.1",
    "remix-auth-github": "^1.6.0",
    "remix-auth-google": "^1.2.0",
    "remix-themes": "^1.2.1",
    "remix-utils": "^6.6.0",
    "slugify": "^1.6.6",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.0.0",
    "@remix-run/eslint-config": "^2.0.0",
    "@types/ramda": "^0.29.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "esbuild": "^0.19.3",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  },
  "overrides": {
    "remix-auth": {
      "@remix-run/react": "$@remix-run/react",
      "@remix-run/server-runtime": "$@remix-run/server-runtime"
    },
    "remix-auth-github": {
      "@remix-run/react": "$@remix-run/react",
      "@remix-run/server-runtime": "$@remix-run/server-runtime"
    },
    "remix-auth-google": {
      "@remix-run/react": "$@remix-run/react",
      "@remix-run/server-runtime": "$@remix-run/server-runtime"
    },
    "remix-themes": {
      "@remix-run/react": "$@remix-run/react",
      "@remix-run/server-runtime": "$@remix-run/server-runtime"
    },
    "remix-utils": {
      "@remix-run/react": "$@remix-run/react",
      "@remix-run/server-runtime": "$@remix-run/server-runtime"
    }
  }
}