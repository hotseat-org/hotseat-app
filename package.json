{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run prisma:generate && npm run build:css && remix build && npm run build:handler",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "build:handler": "esbuild lambda/handler.ts --platform=node --target=node16 --format=cjs --outfile=\"build/handler.js\"",
    "build:lambda": "npm run build && cp package.json package-lock.json app/repository/prisma/schema.prisma build/ && cd build && npm i --production && npx prisma generate --schema=./schema.prisma",
    "dev": "concurrently \"npm run dev:css\" \"node -r dotenv/config ./node_modules/.bin/remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production node -r dotenv/config ./node_modules/.bin/remix-serve build",
    "prisma:generate": "npx prisma generate --schema=./app/repository/prisma/schema.prisma",
    "prisma:reset": "npx prisma migrate reset --schema=./app/repository/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=./app/repository/prisma/schema.prisma",
    "db:init": "docker run -p 3306:3306 --name qseat-db -e MYSQL_ROOT_PASSWORD=qseat -e MYSQL_DATABASE=qseat -d mysql:8.0.32",
    "db:start": "docker start qseat-db",
    "typecheck": "tsc --project ./tsconfig.json",
    "deploy": "npx cdk --app \"npx ts-node ./aws/bin/deploy.ts\" deploy --all --require-approval never --ci"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.17",
    "@material-tailwind/react": "^1.4.2",
    "@prisma/client": "^4.12.0",
    "@remix-run/architect": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "@remix-run/serve": "^1.15.0",
    "@smplrspace/smplr-loader": "^2.6.0",
    "chalk": "^5.0.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.8",
    "dotenv": "^16.0.0",
    "nanoid": "^3.3.1",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "ramda": "^0.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix": "^1.15.0",
    "remix-auth": "^3.4.0",
    "remix-auth-socials": "^2.0.5",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@aws-cdk/aws-lambda": "^1.198.1",
    "@aws-cdk/aws-s3": "^1.198.1",
    "@aws-cdk/aws-s3-deployment": "^1.198.1",
    "@aws-cdk/core": "^1.198.1",
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@remix-run/node": "^1.15.0",
    "@types/ramda": "^0.28.0",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^8.0.1",
    "esbuild": "^0.17.15",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.5.0",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.6.0",
    "typescript": "^5.0.3"
  },
  "engines": {
    "node": ">=18"
  }
}
