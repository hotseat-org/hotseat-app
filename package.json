{
  "name": "remix-app-template",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run prisma:generate && remix build",
    "dev": "node -r dotenv/config ./node_modules/.bin/remix dev",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production node -r dotenv/config ./node_modules/.bin/remix-serve build",
    "prisma:generate": "npx prisma generate --schema=./app/repositories/main-repository/prisma/schema.prisma",
    "prisma:reset": "npx prisma migrate reset --schema=./app/repositories/main-repository/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema=./app/repositories/main-repository/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=./app/repositories/main-repository/prisma/schema.prisma",
    "db:init": "docker run -p 3306:3306 --name qseat-db -e MYSQL_ROOT_PASSWORD=qseat -e MYSQL_DATABASE=qseat -d mysql:8.0.32",
    "db:start": "docker start qseat-db",
    "typecheck": "tsc --project ./tsconfig.json"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@material-tailwind/react": "^2.1.1",
    "@nextui-org/react": "^2.1.7",
    "@prisma/client": "^5.2.0",
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "@remix-run/vercel": "^1.19.3",
    "@smplrspace/smplr-loader": "^2.8.4",
    "@vercel/remix": "^1.19.3",
    "chalk": "^5.3.0",
    "clsx": "^2.0.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.16.2",
    "isbot": "latest",
    "lucide-react": "^0.272.0",
    "luxon": "^3.4.2",
    "nanoid": "^4.0.2",
    "prisma": "^5.2.0",
    "ramda": "^0.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix": "^1.19.3",
    "remix-auth": "^3.5.1",
    "remix-auth-socials": "^2.0.5",
    "remix-themes": "^1.2.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@types/luxon": "^3.3.1",
    "@types/ramda": "^0.29.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^8.2.1",
    "esbuild": "^0.19.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  }
}